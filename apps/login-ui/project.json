{
  "name": "login-ui",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "agamis",
  "sourceRoot": "apps/login-ui/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/angular:webpack-browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/login-ui",
        "index": "apps/login-ui/src/index.html",
        "main": "apps/login-ui/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/login-ui/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/login-ui/src/favicon.ico",
          "apps/login-ui/src/assets",
          "apps/login-ui/src/mockServiceWorker.js"
        ],
        "styles": [
          "node_modules/normalize.css/normalize.css",
          "apps/login-ui/src/styles.scss"
        ],
        "scripts": [],
        "customWebpackConfig": {
          "path": "apps/login-ui/webpack.config.ts"
        }
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all",
          "fileReplacements": [
            {
              "replace": "apps/login-ui/src/environments/environment.ts",
              "with": "apps/login-ui/src/environments/environment.prod.ts"
            }
          ],
          "customWebpackConfig": {
            "path": "apps/login-ui/webpack.prod.config.ts"
          }
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nx/angular:dev-server",
      "options": {
        "port": 4201,
        "publicHost": "http://localhost:4201"
      },
      "configurations": {
        "production": {
          "buildTarget": "login-ui:build:production"
        },
        "development": {
          "buildTarget": "login-ui:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "login-ui:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/login-ui/jest.config.ts",
        "codeCoverage": true
      },
      "configurations": {
        "ci": {
          "testResultsProcessor": "jest-sonar-reporter",
          "coverageReporters": ["clover", "json", "lcov", "text", "text-summary"],
          "collectCoverage": true,
          "coverageDirectory": "{workspaceRoot}/coverage/{projectRoot}"
        }
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "defaultConfiguration": "production",
      "options": {
        "buildTarget": "login-ui:build",
        "port": 4201,
        "watch": false
      },
      "configurations": {
        "development": {
          "buildTarget": "login-ui:build:development"
        },
        "production": {
          "buildTarget": "login-ui:build:production"
        }
      }
    },
    "storybook": {
      "executor": "@storybook/angular:start-storybook",
      "options": {
        "port": 4400,
        "configDir": "apps/login-ui/.storybook",
        "browserTarget": "login-ui:build-storybook",
        "compodoc": false,
        "styles": [
          "node_modules/normalize.css/normalize.css",
          "apps/login-ui/src/styles.scss"
        ]
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@storybook/angular:build-storybook",
      "outputs": ["{options.outputDir}"],
      "options": {
        "outputDir": "dist/storybook/login-ui",
        "configDir": "apps/login-ui/.storybook",
        "browserTarget": "login-ui:build-storybook",
        "compodoc": false,
        "styles": [
          "node_modules/normalize.css/normalize.css",
          "apps/login-ui/src/styles.scss"
        ]
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "test-storybook": {
      "executor": "nx:run-commands",
      "options": {
        "command": "test-storybook -c apps/login-ui/.storybook --url=http://localhost:4400"
      }
    },
    "static-storybook": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "login-ui:build-storybook",
        "staticFilePath": "dist/storybook/login-ui"
      },
      "configurations": {
        "ci": {
          "buildTarget": "login-ui:build-storybook:ci"
        }
      }
    }
  }
}
